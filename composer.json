{
    "name": "craftsys/laravel-redis-session-enhanced",
    "description": "Enhanced redis driver for sessions in Laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Sudhir Mitharwal",
            "email": "luckysud4@gmail.com",
            "role": "Developer",
            "homepage": "http://github.com/sudkumar"
        }
    ],
    "keywords": [
        "php",
        "laravel",
        "session",
        "redis"
    ],
    "homepage": "https://github.dev/RikoDEV/laravel-redis-session-enhanced",
    "support": {
        "issues": "https://github.dev/RikoDEV/laravel-redis-session-enhanced/issues",
        "source": "https://github.dev/RikoDEV/laravel-redis-session-enhanced"
    },
    "require": {
        "php": "^8.2|^8.3",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^8.0|^9.4|^10.0|^11.0|^12.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Craftsys\\LaravelRedisSessionEnhanced\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Craftsys\\Tests\\LaravelRedisSessionEnhanced\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x.x-dev"
        },
        "laravel": {
            "providers": [
                "Craftsys\\LaravelRedisSessionEnhanced\\RedisSessionEnhancedServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit --colors=always"
    }
}
